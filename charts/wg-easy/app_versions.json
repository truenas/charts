            "annotations": {
                "title": "WG Easy"
            },
            "type": "application",
            "version": "1.0.12",
            "apiVersion": "v2",
            "appVersion": "7",
            "kubeVersion": ">=1.16.0-0",
            "maintainers": [
                {
                    "name": "truenas",
